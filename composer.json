{
    "name": "eaxis/htmlsanitizer",
    "description": "An extension to sanitize your Bolt articles.",
    "type": "bolt-extension",
    "keywords": [
        "bolt",
        "articles",
        "sanitize"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "eaxis"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\eaxis\\HtmlSanitizer\\": "src"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\eaxis\\HtmlSanitizer\\HtmlSanitizerExtension"
    }
}
